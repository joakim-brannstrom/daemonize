dependency "daemonize" path="../.."

configuration "daemon" {
    name "daemon"
    targetType "executable"
    targetName "ex2-daemon"
    versions "DaemonServer"
}

configuration "client" {
    name "client"
    targetType "executable"
    targetName "ex2-client"
    versions "DaemonClient"
}
